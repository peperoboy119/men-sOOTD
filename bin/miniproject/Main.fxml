<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<GridPane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="768.0"
	prefWidth="1024.0" style="-fx-background-color: white;"
	xmlns="http://javafx.com/javafx/8.0.171"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="miniproject.Controller">
	<columnConstraints>
		<ColumnConstraints hgrow="SOMETIMES"
			maxWidth="333.0" minWidth="10.0" prefWidth="267.0" />
		<ColumnConstraints hgrow="SOMETIMES"
			maxWidth="245.0" minWidth="10.0" prefWidth="245.0" />
		<ColumnConstraints hgrow="SOMETIMES"
			maxWidth="273.0" minWidth="10.0" prefWidth="273.0" />
		<ColumnConstraints hgrow="SOMETIMES"
			maxWidth="328.0" minWidth="10.0" prefWidth="307.0" />
	</columnConstraints>
	<rowConstraints>
		<RowConstraints maxHeight="129.0" minHeight="10.0"
			prefHeight="121.0" vgrow="SOMETIMES" />
		<RowConstraints maxHeight="56.0" minHeight="10.0"
			prefHeight="39.0" vgrow="SOMETIMES" />
		<RowConstraints maxHeight="625.0" minHeight="10.0"
			prefHeight="580.0" vgrow="SOMETIMES" />
		<RowConstraints maxHeight="461.0" minHeight="10.0"
			prefHeight="30.0" vgrow="SOMETIMES" />
	</rowConstraints>
	<children>
		<Label ellipsisString="" style="-fx-background-color: white;"
			text="Men's OOTD" GridPane.columnIndex="1" GridPane.columnSpan="2"
			GridPane.halignment="CENTER">
			<font>
				<Font name="System Bold" size="48.0" />
			</font>
		</Label>
		<HBox alignment="CENTER" prefHeight="11.0" prefWidth="1024.0"
			style="-fx-background-color: black;" GridPane.columnSpan="4"
			GridPane.halignment="CENTER" GridPane.rowIndex="1"
			GridPane.valignment="CENTER">
			<children>
				<VBox prefHeight="200.0" prefWidth="200.0">
					<children>
						<MenuButton fx:id="topMenu" alignment="CENTER"
							mnemonicParsing="false" prefHeight="35.0" prefWidth="200.0"
							style="-fx-background-color: black;" text="상의" textFill="WHITE">
							<items>
								<MenuItem fx:id="top1" mnemonicParsing="false"
									onAction="#openDetail1" style="-fx-padding: 0 68;" text="맨투맨" />
								<MenuItem fx:id="top2" mnemonicParsing="false"
									onAction="#openDetail1" style="-fx-padding: 0 68;" text="셔츠" />
								<MenuItem fx:id="top3" mnemonicParsing="false"
									onAction="#openDetail1" style="-fx-padding: 0 68;" text="니트" />
							</items>
							<font>
								<Font name="System Bold" size="14.0" />
							</font>
						</MenuButton>
					</children>
				</VBox>
				<VBox prefHeight="200.0" prefWidth="200.0">
					<children>
						<MenuButton alignment="CENTER" mnemonicParsing="false"
							prefHeight="35.0" prefWidth="200.0"
							style="-fx-background-color: black;" text="하의" textFill="WHITE">
							<items>
								<MenuItem mnemonicParsing="false"
									onAction="#openDetail1" style="-fx-padding: 0 60;" text="청바지" />
								<MenuItem mnemonicParsing="false"
									onAction="#openDetail1" style="-fx-padding: 0 60;" text="골덴바지" />
								<MenuItem mnemonicParsing="false"
									onAction="#openDetail1" style="-fx-padding: 0 60;" text="슬랙스" />
							</items>
							<font>
								<Font name="System Bold" size="14.0" />
							</font>
						</MenuButton>
					</children>
				</VBox>
				<VBox prefHeight="200.0" prefWidth="200.0">
					<children>
						<MenuButton alignment="CENTER" mnemonicParsing="false"
							prefHeight="35.0" prefWidth="200.0"
							style="-fx-background-color: black;" text="아우터" textFill="WHITE">
							<items>
								<MenuItem mnemonicParsing="false"
									onAction="#openDetail1" style="-fx-padding: 0 50;" text="숏패딩" />
								<MenuItem mnemonicParsing="false"
									onAction="#openDetail1" style="-fx-padding: 0 50;"
									text="라이더 자켓" />
								<MenuItem mnemonicParsing="false"
									onAction="#openDetail1" style="-fx-padding: 0 50;" text="울코트" />
							</items>
							<font>
								<Font name="System Bold" size="14.0" />
							</font>
						</MenuButton>
					</children>
				</VBox>
				<VBox prefHeight="200.0" prefWidth="200.0">
					<children>
						<MenuButton alignment="CENTER" mnemonicParsing="false"
							prefHeight="35.0" prefWidth="200.0"
							style="-fx-background-color: black;" text="신발" textFill="WHITE">
							<items>
								<MenuItem mnemonicParsing="false"
									onAction="#openDetail1" style="-fx-padding: 0 53;" text="로퍼" />
								<MenuItem mnemonicParsing="false"
									onAction="#openDetail1" style="-fx-padding: 0 53;" text="어글리슈즈" />
								<MenuItem mnemonicParsing="false"
									onAction="#openDetail1" style="-fx-padding: 0 53;" text="단화" />
							</items>
							<font>
								<Font size="14.0" />
							</font>
						</MenuButton>
					</children>
				</VBox>
			</children>
			<GridPane.margin>
				<Insets />
			</GridPane.margin>
		</HBox>
		<Label text="(주) Men's OOTD" GridPane.columnIndex="1"
			GridPane.columnSpan="2" GridPane.halignment="CENTER"
			GridPane.rowIndex="3">
			<font>
				<Font size="18.0" />
			</font>
			<GridPane.margin>
				<Insets bottom="10.0" />
			</GridPane.margin>
		</Label>
		<HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0"
			GridPane.columnIndex="3" GridPane.rowIndex="3" />
		<HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0"
			spacing="5.0" GridPane.columnIndex="3">
			<children>
				<Button fx:id="mini" mnemonicParsing="false"
					onAction="#minimize" style="-fx-background-color: black;"
					text="최소화" textFill="WHITE">
					<font>
						<Font size="13.0" />
					</font>
				</Button>
				<Button fx:id="loginbtn" mnemonicParsing="false"
					onAction="#openLogin" style="-fx-background-color: black;"
					text="로그인" textFill="WHITE">
					<font>
						<Font size="13.0" />
					</font>
				</Button>
				<Button fx:id="mypagebtn" mnemonicParsing="false"
					onAction="#openMyPage" style="-fx-background-color: black;"
					text="마이페이지" textFill="WHITE">
					<font>
						<Font size="13.0" />
					</font>
				</Button>
			</children>
		</HBox>
		<HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0"
			GridPane.columnSpan="4" GridPane.rowIndex="2">
			<children>
				<ImageView fitHeight="537.0" fitWidth="700.0"
					pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="@img/shopmain.jpg" />
					</image>
				</ImageView>
			</children>
			<GridPane.margin>
				<Insets top="10.0" />
			</GridPane.margin>
		</HBox>
	</children>
</GridPane>
